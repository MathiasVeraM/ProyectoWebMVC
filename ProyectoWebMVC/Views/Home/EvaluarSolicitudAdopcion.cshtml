@model IEnumerable<ProyectoWebMVC.Models.Solicitud>

@{
    ViewData["Title"] = "EvaluarSolicitudAdopcion";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Solicitudes enviadas</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Cedula)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Edad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LugarResidencia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ingresos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AmbienteFamiliar)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fecha)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayNameFor(modelItem => item.Cedula)
                </td>
                <td>
                    @Html.DisplayNameFor(modelItem => item.Nombre)
                </td>
                <td>
                    @Html.DisplayNameFor(modelItem => item.Edad)
                </td>
                <td>
                    @Html.DisplayNameFor(modelItem => item.LugarResidencia)
                </td>
                <td>
                    @Html.DisplayNameFor(modelItem => item.Ingresos)
                </td>
                <td>
                    @Html.DisplayNameFor(modelItem => item.AmbienteFamiliar)
                </td>
                <td>
                    @Html.DisplayNameFor(modelItem => item.Fecha)
                </td>
            </tr>
        }
    </tbody>
</table>
